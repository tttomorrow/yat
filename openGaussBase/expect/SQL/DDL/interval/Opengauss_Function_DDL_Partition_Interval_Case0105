-- @testpoint: interval分区,EXCHANGE PARTITION分区表被置换的分区是预定义分区
drop table if exists pt1;
SQL SUCCESS
drop table if exists common_table1;
SQL SUCCESS
create table pt1(
col_1 smallint,
col_2 char(30),
col_3 int,
col_4 date not null,
col_5 boolean,
col_6 nchar(30),
col_7 float
)
partition by range (col_4)
interval ('1 month')
(
partition pt1_p1 values less than ('2020-01-01'));
SQL SUCCESS
create table common_table1(
col_1 smallint,
col_2 char(30),
col_3 int,
col_4 date not null,
col_5 boolean,
col_6 nchar(30),
col_7 float);
SQL SUCCESS
insert into pt1 values (1,'aaa',1,'2019-12-31',true,'aaa',1.1);
SQL SUCCESS
insert into common_table1 values (1,'aaa',1,'2019-12-15',true,'aaa',1.1);
SQL SUCCESS
select * from pt1;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
select * from common_table1;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
alter table pt1 exchange partition (pt1_p1) with table common_table1;
SQL SUCCESS
select * from pt1;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
select * from common_table1;
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
| col_1 | col_2                          | col_3 | col_4                         | col_5 | col_6                          | col_7 |
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
+-------+--------------------------------+-------+-------------------------------+-------+--------------------------------+-------+
drop table if exists pt1;
SQL SUCCESS
drop table if exists common_table1;
SQL SUCCESS
