--删除表数据并删除多列
delete  temp_table_alter_011;
SQL SUCCESS
alter table temp_table_alter_011 drop c_1;
SQL SUCCESS
alter table temp_table_alter_011 drop c_decimal;
SQL SUCCESS
alter table temp_table_alter_011 drop  c_date;
SQL SUCCESS
--删除表
drop table temp_table_alter_011;
SQL SUCCESS
