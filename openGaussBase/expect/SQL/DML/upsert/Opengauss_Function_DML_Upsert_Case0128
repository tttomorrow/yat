insert into t_dml_upsert0128 (a,b,c) select a,b,substring(c,3,1) from t_dml_upsert_sub0128 where b<10 on duplicate key update d = (select c from t_dml_upsert_sub0128 where a = excluded.a and b= excluded.b);
