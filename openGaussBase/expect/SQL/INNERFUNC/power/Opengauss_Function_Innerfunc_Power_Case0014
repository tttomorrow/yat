-- @testpoint: power函数,参数为有效数值，第二个参数为-1
select power(3,-1) as result from sys_dummy;
+--------------------+
| result             |
+--------------------+
| 0.3333333333333333 |
+--------------------+
select power(4.00,-1) as result from sys_dummy;
+--------------------+
| result             |
+--------------------+
+--------------------+
