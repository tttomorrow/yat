-- @testpoint: Jsonb高级特性：num-jsonb类型大小比较：数值比较
-- = 相等
select '123.0'::jsonb = '123'::jsonb;
+----------+
| ?column? |
+----------+
| true     |
+----------+
+----------+
| ?column? |
+----------+
| true     |
+----------+
select '1235e-5'::jsonb = '1.235e-2'::jsonb;
+----------+
| ?column? |
+----------+
| true     |
+----------+
select '-789'::jsonb = '123'::jsonb;
+----------+
| ?column? |
+----------+
| false    |
+----------+
select '110'::jsonb = '-119'::jsonb;
+----------+
| ?column? |
+----------+
| false    |
+----------+
-- <> 不相等
select '123.0'::jsonb <> '123'::jsonb;
+----------+
| ?column? |
+----------+
| false    |
+----------+
+----------+
| ?column? |
+----------+
| false    |
+----------+
