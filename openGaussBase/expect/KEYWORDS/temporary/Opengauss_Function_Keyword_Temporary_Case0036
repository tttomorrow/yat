--  @testpoint:openGauss关键字temporary(非保留)
-- 创建本地临时表，并指定提交事务时删除该临时表数据
DROP TABLE  IF EXISTS table_test2;
SQL SUCCESS
CREATE TEMPORARY TABLE table_test2 ( W_WAREHOUSE_SK INTEGER NOT NULL, W_WAREHOUSE_ID CHAR(16) NOT NULL, W_WAREHOUSE_NAME VARCHAR(20) , W_WAREHOUSE_SQ_FT INTEGER , W_STREET_NUMBER CHAR(10) , W_STREET_NAME VARCHAR(60) , W_STREET_TYPE CHAR(15) , W_SUITE_NUMBER CHAR(10) , W_CITY VARCHAR(60) , W_COUNTY VARCHAR(30) , W_STATE CHAR(2) , W_ZIP CHAR(10) , W_COUNTRY VARCHAR(20) , W_GMT_OFFSET DECIMAL(5,2) ) ON COMMIT DELETE ROWS;
SQL SUCCESS
DROP TABLE  table_test2;
SQL SUCCESS
